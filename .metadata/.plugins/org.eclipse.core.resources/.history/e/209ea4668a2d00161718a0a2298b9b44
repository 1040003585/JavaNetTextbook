package _4_3Socket编程示例._4_4_3CS环境下的套接字应用程序;

import _6_001多线程问题.Mother;

public class Son extends Mother {
	String name="...";

	public Son(String name) {
		super(name);
		this.name = name;
	}

//	@Override
	public void run() {
		System.out.println(name);
	}

	public void startup() {
		super.start();//son的start()
		super.run();
	}
}