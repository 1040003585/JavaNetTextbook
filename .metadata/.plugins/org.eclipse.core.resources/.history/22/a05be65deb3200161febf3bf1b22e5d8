package JavaLightWeightHttpProxy;

public class TestDaemon {
	public static  int count=0;
	public static void main(String[] args) {
		
		Thread thread = new Thread(
			new Runnable() {
				public void run() {
					count++;
					System.out.println(count);
				}
			}
		);
		thread.setDaemon(true);
		thread.start();
	}
}
