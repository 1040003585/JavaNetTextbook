import java.net.*;
public class URL {

	public static void main(String[] args) {
		// 1
		try {
			URL name = new URL("http://www.baidu.com");
			
		} catch (MalformedURLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		//2
		URL name = new URL(arguments);
		

	}

}